{
  "name": "lucky-16-cards-app",
  "version": "1.0.0",
  "description": "lucky 16 cards game application frontend UI",
  "main": "app.js",
  "author": "Vspatil",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "electron": "^7.1.2",
    "electron-builder": "^10.17.3",
    "electron-packager": "^15.4.0"
  },
  "license": "ISC",
  "build": {
    "asar": true,
    "appId": "com.smasher.lucky16cards",
    "productName": "lucky16cards-H",
    "win": {
      "icon": "./src/assets/images/a2.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "extraFiles": [
      "resolu.exe"
    ],
    "nsis": {
      "createDesktopShortcut": "always"
    },
    "files": [
      "./**/*"
    ],
    "extraMetadata": {
      "main": "app/index.js"
    },
    "extends": null
  },
  "dependencies": {
    "dateformat": "^4.5.1",
    "electron-pos-printer": "^1.2.0",
    "jquery": "^3.7.0",
    "jsbarcode": "^3.11.5",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "shell": "^0.8.4",
    "socket.io-client": "^4.1.3",
    "v8-compile-cache": "^2.3.0"
  }
}
